package com.mindfunction.smirkengine.collider
{
	import flash.events.Event;
	
	public class CollidedEvent extends Event
	{
		
		public var targetCollider:ColliderBehaviour;
		
		public function CollidedEvent(type:String, t:ColliderBehaviour, bubbles:Boolean=false, cancelable:Boolean=false)
		{
			super(type, bubbles, cancelable);
			targetCollider=t;
		}
	}
}