package com.mindfunction.smirkengine
{
	import flash.events.Event;
	
	public class BehaviourRemovedEvent extends Event
	{
		
		public var behaviour:EntityBehaviour;
		
		public function BehaviourRemovedEvent(type:String, b:EntityBehaviour, bubbles:Boolean=false, cancelable:Boolean=false)
		{
			super(type, bubbles, cancelable);
			behaviour=b;
		}
	}
}