package smirkengine
{
	import flash.events.Event;
	
	public class EntityAddedEvent extends Event
	{
		
		public var entity:Entity;
		
		public function EntityAddedEvent(type:String, e:Entity, bubbles:Boolean=false, cancelable:Boolean=false)
		{
			super(type, bubbles, cancelable);
			entity=e;
		}
	}
}