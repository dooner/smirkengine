package com.mindfunction.geditor.model.interfaces
{
	public interface IModel
	{
		function get id():String;
		function get childs():Array;
		function addChild(m:IModel):void;
		function removeChild(m:IModel):void;
		function childByName(s:String):IModel;
		function get name():String;
		function set name(n:String);
		function get type():IModel;
		function set type(t:IModel);
		function get data():XML;
	}
}